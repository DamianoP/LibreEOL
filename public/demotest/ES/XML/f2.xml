</itemmetadata>
<presentation>
<material>
<mattext texttype="text/html"><![CDATA[<P>Ordénense las moléculas: CH<SUB>4</SUB>, CH<SUB>3</SUB>F, CH<SUB>2</SUB>F<SUB>2</SUB> y CF<SUB>4</SUB>, por orden creciente de su momento dipolar:<BR></P>]]></mattext>
</material>
<response_lid ident="1">
<render_choice shuffle="Yes">
<response_label ident="A">
<material>
<mattext texttype="text/html"><![CDATA[CH<SUB>3</SUB>F  &gt;  CH<SUB>2</SUB>F<SUB>2</SUB>  &gt;  CF<SUB>4 </SUB>  &gt;  CH<SUB>4</SUB><BR>]]></mattext>
</material>
</response_label>
<response_label ident="B">
<material>
<mattext texttype="text/html"><![CDATA[CH<SUB>2</SUB>F<SUB>2</SUB>  &gt;  CF<SUB>4 </SUB>  &gt;  CH<SUB>3</SUB>F  &gt;  CH<SUB>4</SUB><BR>]]></mattext>
</material>
</response_label>
<response_label ident="C">
<material>
<mattext texttype="text/html"><![CDATA[CH<SUB>4  </SUB>  =  CF<SUB>4 </SUB>  &gt;  CH<SUB>3</SUB>F  &gt;  CH<SUB>2</SUB>F<SUB>2</SUB><BR>]]></mattext>
</material>
</response_label>
<response_label ident="D">
<material>
<mattext texttype="text/html"><![CDATA[CF<SUB>4</SUB>  &gt;  CH<SUB>2</SUB>F<SUB>2</SUB>  &gt;  CH<SUB>3</SUB>F  &gt;  CH<SUB>4</SUB><BR>]]></mattext>
</material>
</response_label>
<response_label ident="E">
<material>
<mattext texttype="text/html"><![CDATA[CH<SUB>2</SUB>F<SUB>2</SUB>  &gt;  CH<SUB>3</SUB>F  &gt;  CH<SUB>4</SUB>  =  CF<SUB>4</SUB><BR>]]></mattext>
</material>
</response_label>
</render_choice>
